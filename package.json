{
  "name": "habits",
  "displayName": "Habits",
  "description": "A silly little extensio to keep track of my bad habits while coding in hope to get rid of them",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",

  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Habits-menu",
          "title": "Habits",
          "icon": "assets/penguin.svg"
        }
      ]
    },
    "views": {
      "Habits-menu": [
        {
          "id": "package-dependencies",
          "name": "Dependencies"
        },
        {
          
          "id": "package-outline",
          "name": "Outline"
        }
      ]
    },
    "commands": [
      {
        
        "command": "habits.helloWorld",
        "title": "Hello World"
      },
      {
        "command" : "habits.SEXOTILIN",
        "title": "SEXOTILIN"
      }
    ]
  },

  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}